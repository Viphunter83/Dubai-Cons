# .env.example

# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5432/dubaicons
REDIS_URL=redis://localhost:6379/0

# AI Services
OPENAI_API_KEY=your_openai_api_key_here
STABILITY_AI_API_KEY=your_stability_ai_api_key_here
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Payment Processing
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Email Services
SENDGRID_API_KEY=your_sendgrid_api_key
FROM_EMAIL=noreply@dubaicons.ai

# WhatsApp Business API
WHATSAPP_ACCESS_TOKEN=your_whatsapp_access_token
WHATSAPP_PHONE_NUMBER_ID=your_phone_number_id

# Cloud Storage
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_S3_BUCKET_NAME=dubaicons-storage
AWS_REGION=us-east-1

# Security
SECRET_KEY=your_secret_key_here
JWT_SECRET_KEY=your_jwt_secret_key
ENCRYPTION_KEY=your_encryption_key

# Application Settings
APP_NAME=Dubai Cons AI Suite
APP_VERSION=1.0.0
DEBUG=True
LOG_LEVEL=INFO
CORS_ORIGINS=http://localhost:3000,http://localhost:8080

# External APIs
DUBAI_SUPPLIERS_API_KEY=your_dubai_suppliers_api_key
AUTODESK_API_KEY=your_autodesk_api_key
GOOGLE_ANALYTICS_ID=your_ga_id

# Monitoring
SENTRY_DSN=your_sentry_dsn
PROMETHEUS_PORT=8001

# Development
ENVIRONMENT=development
RELOAD=True
WORKERS=1
